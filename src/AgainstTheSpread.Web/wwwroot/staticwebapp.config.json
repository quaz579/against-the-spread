{
  "auth": {
    "identityProviders": {
      "google": {
        "registration": {
          "clientIdSettingName": "GOOGLE_CLIENT_ID",
          "clientSecretSettingName": "GOOGLE_CLIENT_SECRET"
        }
      }
    }
  },
  "routes": [
    {
      "route": "/api/upload-lines",
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/.auth/login/google",
      "statusCode": 200
    },
    {
      "route": "/.auth/logout",
      "statusCode": 200
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/.auth/login/google",
      "statusCode": 302
    }
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/images/*.{png,jpg,gif}",
      "/css/*",
      "/.auth/*",
      "/api/*"
    ]
  }
}